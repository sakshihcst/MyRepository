package com.searshc.targetinteraction;

import java.io.Serializable;

import org.codehaus.jackson.annotate.JsonIgnoreProperties;

@JsonIgnoreProperties(ignoreUnknown=true)
public class PriceDetailsDTO implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 2483377751365457926L;
	
	private double amount;
	private String type;
	private int priceMultiple;
	private String priceMessage;
	public double getAmount() {
		return amount;
	}
	public void setAmount(double amount) {
		this.amount = amount;
	}
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public int getPriceMultiple() {
		return priceMultiple;
	}
	public void setPriceMultiple(int priceMultiple) {
		this.priceMultiple = priceMultiple;
	}
	public String getPriceMessage() {
		return priceMessage;
	}
	public void setPriceMessage(String priceMessage) {
		this.priceMessage = priceMessage;
	}
	
}
